f (this.type === 'error') {\n        return 'bg-red-600';\n      } else if (this.type === 'confirm') {\n        return 'bg-orange-500';\n      }\n      return 'bg-green-600';\n    }\n  }\n};\n</script>\n\n<style scoped>\ndialog {\n  top: 10vh;\n  left: 10%;\n}\n\n@media (min-width: 768px) {\n  dialog {\n    left: calc(50% - 20rem);\n    width: 40rem;\n  }\n}\n\n.dialog-enter-from,\n.dialog-leave-to {\n  opacity: 0;\n  transform: translateY(-50px) scale(0.8);\n}\n\n.dialog-enter-active {\n  transition: all 0.3s ease-out;\n}\n\n.dialog-leave-active {\n  transition: all 0.3s ease-in;\n}\n\n.dialog-enter-to,\n.dialog-leave-from {\n  opacity: 1;\n  transform: translateY(0) scale(1);\n}\n</style>"],"sourceRoot":""}]}